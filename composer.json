{
    "name": "tobymaxham/log-viewer",
    "description": "A Laravel log reader",
    "license": "MIT",
    "keywords": ["log", "log-reader", "log-viewer", "logging", "zhylon"],
    "authors": [
        {
            "name": "Tobias Maxham",
            "email": "git@maxham.de"
        }
    ],
    "require": {
        "php": "^8.2",
        "rap2hpoutre/laravel-log-viewer": "^2.3.0"
    },
    "autoload": {
        "psr-4": {
            "TobyMaxham\\Logger\\": "src"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "TobyMaxham\\Logger\\LogViewerServiceProvider"
            ]
        }
    }
}
